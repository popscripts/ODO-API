{
  "name": "odoapi",
  "version": "2.0.0",
  "main": "app.js",
  "repository": "https://gitlab.com/specsci/odoapi.git",
  "author": "Bartosz Poszelezny <bartoszposzelezny@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "node dist/src/app.js",
    "dev": "nodemon -r tsconfig-paths/register src/app.ts",
    "build": "ttsc -p .",
    "migrate": "npx prisma migrate dev --name",
    "push": "prisma db push",
    "seed": "node --require esbuild-register prisma/seed.ts",
    "seed:dev": "node --require esbuild-register prisma/devSeed.ts"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "cron": "^3.1.6",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-fileupload": "^1.4.1",
    "express-validator": "^7.0.1",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "moment-timezone": "^0.5.40",
    "morgan": "^1.10.0",
    "socket.io": "^4.7.2",
    "ttsc": "^0.3.1",
    "typescript-transform-paths": "^3.4.6",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.2.0",
    "@prisma/client": "^5.6.0",
    "@types/bcrypt": "^5.0.1",
    "@types/cookie-parser": "^1.4.5",
    "@types/cors": "^2.8.15",
    "@types/express": "^4.17.20",
    "@types/express-fileupload": "^1.4.3",
    "@types/jsonwebtoken": "^9.0.4",
    "@types/morgan": "^1.9.7",
    "@types/multer": "^1.4.9",
    "@types/node": "^20.8.9",
    "esbuild": "^0.19.5",
    "esbuild-register": "^3.5.0",
    "multer": "^1.4.5-lts.1",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.3",
    "prisma": "^5.6.0",
    "ts-node": "^10.9.1",
    "tsc": "^2.0.4",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.2.2"
  },
  "prettier": {
    "tabWidth": 4,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "none",
    "editor.formatOnSave": true,
    "bracketSameLine": false,
    "wordWrap": "wordWrapColumn",
    "printWidth": 80
  }
}
